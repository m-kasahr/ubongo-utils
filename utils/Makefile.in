           ubongo-solve-dir ubongo-solve-3d ubongo-analyze-orig \
           ubongo-analyze-duel ubongo-analyze-minib ubongo-analyze-minig \
           ubongo-analyze-minir ubongo-analyze-miniy ubongo-analyze-dir \
           ubongo-analyze-3d 

SCRIPTS = ubongo-draw ubongo-html-orig ubongo-html-duel ubongo-html-minib \
          ubongo-html-minig ubongo-html-minir ubongo-html-miniy \
          ubongo-html-3d ubongo-compare

DISTFILES = Makefile.in ubongo-solve-x.cpp.in ubongo-analyze-x.cpp.in \
            getopt.c getopt.h ubongo-html-x.in ubongo-html-3d.in \
            ubongo-draw.in ubongo-compare.in 
CLEANFILES = $(SCRIPTS) \
             $(ubongosolveorig_SOURCES) $(ubongosolveduel_SOURCES) \
             $(ubongosolveminib_SOURCES) $(ubongosolveminig_SOURCES) \
             $(ubongosolveminir_SOURCES) $(ubongosolveminiy_SOURCES) \
             $(ubongosolvedir_SOURCES) $(ubongosolve3d_SOURCES) \
             $(ubongoanalyzeorig_SOURCES) $(ubongoanalyzeduel_SOURCES) \
             $(ubongoanalyzeminib_SOURCES) $(ubongoanalyzeminig_SOURCES) \
             $(ubongoanalyzeminir_SOURCES) $(ubongoanalyzeminiy_SOURCES) \
             $(ubongoanalyzedir_SOURCES) $(ubongoanalyze3d_SOURCES)
LIBTOOL_CLEANFILES = $(PROGRAMS) \
             $(ubongosolveorig_OBJECTS) $(ubongosolveduel_OBJECTS) \
             $(ubongosolveminib_OBJECTS) $(ubongosolveminig_OBJECTS) \
             $(ubongosolveminir_OBJECTS) $(ubongosolveminiy_OBJECTS) \
             $(ubongosolvedir_OBJECTS) $(ubongosolve3d_OBJECTS) \
             $(ubongoanalyzeorig_OBJECTS) $(ubongoanalyzeduel_OBJECTS) \
             $(ubongoanalyzeminib_OBJECTS) $(ubongoanalyzeminig_OBJECTS) \
             $(ubongoanalyzeminir_OBJECTS) $(ubongoanalyzeminiy_OBJECTS) \
             $(ubongoanalyzedir_OBJECTS) $(ubongoanalyze3d_OBJECTS)

.SUFFIXES:

.PHONY: all clean distclean install uninstall dist-local

all: $(PROGRAMS) $(SCRIPTS)

install: all
	$(MKDIR_P) $(DESTDIR)$(bindir)
	for i in $(PROGRAMS); do \
	    $(LIBTOOL) --mode=install $(INSTALL_PROGRAM) $$i \
	        $(DESTDIR)$(bindir)/$$i; \
	done
	for i in $(SCRIPTS); do \
	    $(INSTALL_SCRIPT) $$i $(DESTDIR)$(bindir)/$$i; \
	done

uninstall:
	for i in $(PROGRAMS); do \
	    $(LIBTOOL) --mode=uninstall rm -f $(DESTDIR)$(bindir)/$$i; \
	done
	for i in $(SCRIPTS); do \
	    rm -f $(DESTDIR)$(bindir)/$$i; \
	done

clean:
	-$(LIBTOOL) --mode=clean rm -f $(LIBTOOL_CLEANFILES)
	-rm -f $(CLEANFILES)

distclean: clean
	-rm -f Makefile

dist-local:
	$(MKDIR_P) $(top_builddir)/$(PACKAGE)/$(subdir)
	for i in $(DISTFILES); do \
	    cp -p $(srcdir)/$$i $(top_builddir)/$(PACKAGE)/$(subdir)/$$i; \
	done

